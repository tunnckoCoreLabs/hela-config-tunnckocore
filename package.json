{
  "name": "hela-preset-tunnckocore",
  "version": "0.0.0-semantic-release",
  "description": "A [hela][] preset of tasks for @tunncoCore GitHub organization",
  "repository": "tunnckoCore/hela-preset-tunnckocore",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "nspId": "todo",
  "main": "./index.js",
  "license": "MIT",
  "scripts": {
    "start": "hela",
    "test": "yarn start test",
    "precommit": "yarn start precommit",
    "commit": "yarn start commit"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "index.js",
    "config/"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "builtin-modules": "^1.1.1",
    "camelcase": "^4.1.0",
    "eslint": "^4.6.1",
    "nyc": "^11.2.1",
    "pify": "^3.0.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "simple-commit-message": "^3.3.1",
    "uglify-es": "^3.1.0",
    "verb": "verbose/verb#dev",
    "verb-generate-readme": "^0.6.0",
    "year": "^0.2.1"
  },
  "devDependencies": {
    "eslint-config-standard-tunnckocore": "^1.0.7",
    "hela": "^1.0.2",
    "husky": "^0.14.3",
    "semantic-release": "^7.0.2"
  },
  "keywords": [
    "charlike",
    "cli",
    "command line",
    "es2015",
    "eslint",
    "eslint fix",
    "hela",
    "hooks",
    "lint",
    "precommit",
    "prettier",
    "publish",
    "release",
    "runner",
    "semantic",
    "semantic release",
    "standard",
    "task",
    "task runner",
    "tasks",
    "testing"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "mri"
      ],
      "highlight": "hela"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "charlike-cli",
      "execa"
    ]
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
